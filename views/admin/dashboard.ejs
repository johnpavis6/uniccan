<html>

<head>
    <title>Uniccan - Admin - Signin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="/images/logo.png">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css"
        integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <!-- <link rel="stylesheet" href="/css/signin.css"> -->
    <% for(key in users){ %>
    <meta name="users-<%= key %>" content="<%= users[key] %>">
    <% } %>
</head>

<body style="background-image: url(/images/bg.jpeg);background-size: 100% 100%;">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="/#">
            <img src="/images/logo.png" width="30" height="30">
            Uniccan
        </a>
        <a class="btn btn-outline-success" href="/admin/logout">Log out</a>
    </nav>
    <div class="container">
        <table class="table table-hover table-bordered table-responsive">
            <thead class="table-success ">
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Gender</th>
                    <th>Mobile No</th>
                    <th>DOB</th>
                    <th>Education</th>
                    <th>Hobbies</th>
                    <th>Extra curricular activities</th>
                    <th>Involvements</th>
                    <th>Skills</th>
                    <th>Last seen</th>
                    <th>Knowledges</th>
                    <th>College name</th>
                    <th>Work experience</th>
                    <th>Degrees</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < users.length; i++) { %>
                <tr>
                    <td><%= users[i].name %></td>
                    <td><%= users[i].email %></td>
                    <td><%= users[i].gender %></td>
                    <td><%= users[i].mobile_no %></td>
                    <td><%= users[i].dob %></td>
                    <td><%= users[i].education %></td>
                    <td><%= users[i].hobbies %></td>
                    <td><%= users[i].extra_curricular_activities %></td>
                    <td><%= users[i].involvements %></td>
                    <td><%= users[i].skills %></td>
                    <td><%= users[i].last_seen %></td>
                    <td><%= users[i].knowledges %></td>
                    <td><%= users[i].college_name %></td>
                    <td>
                    <table>
                            <thead class="table-success">
                    <th>Company_name</th>
                    <th>Months</th>
                    </thead>
                <tr>
                    <% work_experiences = JSON.parse(users[i].work_experiences) 
                                    for( var key in work_experiences){
                                %>
                    <td><%= work_experiences[key].company_name %></td>
                    <td><%= work_experiences[key].months %></td>
                    <% } %>
                </tr>
        </table>
        </td>
        <td><%= users[i].degrees %></td>
        </tr>
        <% } %>
        </tbody>
        </table>
    </div>
</body>

</html>